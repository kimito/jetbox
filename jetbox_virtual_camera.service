[Unit]
Description=virtual camera for jetbox, RC car control subsystem
After=systemd-modules-load.service nvargus-daemon.service

[Service]
ExecStart=/usr/bin/gst-launch-1.0 -v nvarguscamerasrc ! 'video/x-raw(memory:NVMM), format=NV12, width=1280, height=720, framerate=30/1' ! nvvidconv flip-method=2 ! identity drop-allocation=1 ! 'video/x-raw, width=1280, height=720, format=I420, framerate=30/1' ! v4l2sink device=/dev/video1
User=root

[Install]
WantedBy=multi-user.target